{
  "name": "vscode-rain",
  "displayName": "vscode-rain",
  "description": "Hello World",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.96.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "vscode-rain.deploy",
        "title": "Rain: Deploy with active file"
      },
      {
        "command": "vscode-rain.refreshView",
        "title": "Rain: Refresh View",
        "icon": {
          "light": "resources/light/refresh.svg",
          "dark": "resources/dark/refresh.svg"
        }
      },
      {
        "command": "vscode-rain.deployStack",
        "title": "Edit",
        "icon": {
          "light": "resources/light/cloud-upload.svg",
          "dark": "resources/dark/cloud-upload.svg"
        }
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "vscode-rain-sidebar",
          "title": "Rain",
          "icon": "resources/rain-icon.svg"
        }
      ]
    },
    "views": {
      "vscode-rain-sidebar": [
        {
          "id": "vscode-rain-view",
          "name": "Rain"
        }
      ]
    },
    "menus": {
      "view/title": [
        {
          "command": "vscode-rain.refreshView",
          "group": "navigation"
        }
      ],
      "view/item/context": [
        {
          "command": "vscode-rain.deployStack",
          "group": "inline"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.96.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@typescript-eslint/eslint-plugin": "^8.17.0",
    "@typescript-eslint/parser": "^8.17.0",
    "eslint": "^9.16.0",
    "typescript": "^5.7.2",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1"
  }
}
